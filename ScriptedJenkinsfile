node("ansible_docker"){
    stage("Git checkout"){
        git credentialsId: '856bea26-b95b-453b-8c11-1fbddb119645', url: 'git@github.com:aragastmatb/example-playbook.git'
    }
    stage("Check ssh key"){
        secret_check=true
    }
    stage("Run playbook"){
        if (secret_check){
            sh 'ansible-playbook site.yml -i inventory/prod.yml'
        }
        else{
            echo 'no more keys'
        }

    }
}
